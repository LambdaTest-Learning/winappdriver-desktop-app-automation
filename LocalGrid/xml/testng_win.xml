<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="[Demo] Desktop Application Testing using WinAppDriver">
<listeners>
	<listener class-name="org.uncommons.reportng.HTMLReporter"/>
	<listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
</listeners>

<test name="[Demo] Automation of actions on Calculator and Notepad Applications"/>
	<test name="Calculator">
		<classes>
			<class name="WinAppCalcDemo"> </class>
		</classes>
	</test>
	<test name="ClassicCalculator">
		<classes>
			<class name="WinAppClassicCalc"> </class>
		</classes>
	</test>
	<test name="Notepad">
		<parameter name="filePath" value="C:\Himanshu\WinAppDriverDemo.txt"/>
		<classes>
			<class name="WinAppNotepadDemo"> </class>
		</classes>
	</test>

</suite>
